# Environment Variables for Backend Development
# Copy this file to .env and fill in the values

# Server Configuration
NODE_ENV=development
PORT=8080

# GCP Configuration
GCP_PROJECT=autosquare-prod
GCP_REGION=us-central1

# BigTable Configuration (optional for now)
BIGTABLE_INSTANCE=autosquare-sessions
BIGTABLE_TABLE=diagnosis_sessions

# AI Provider (openai or vertexai)
AI_PROVIDER=openai
OPENAI_MODEL=gpt-4o-mini
OPENAI_API_KEY=your-openai-api-key-here

# Vertex AI (if using instead of OpenAI)
VERTEXAI_MODEL=gemini-1.5-flash

# Feature Flags
ENABLE_SESSION_STORAGE=false
ENABLE_CACHING=false
